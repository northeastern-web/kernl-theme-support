{
  "name": "northeastern-web/kernl-theme-support",
  "description": "Package of support classes for Northeastern University websites",
  "keywords": [
    "northeastern-web",
    "support"
  ],
  "homepage": "https://github.com/northeastern-web/support",
  "license": "MIT",
  "authors": [
    {
      "name": "Northeastern Web Solutions",
      "email": "websolutions@northeastern.edu",
      "homepage": "https://northeastern.edu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "Kernl\\Support\\Theme\\": "src"
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
